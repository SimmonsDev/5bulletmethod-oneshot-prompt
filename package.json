{
  "name": "5bulletmethod",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "install:all": "npm --prefix api install && npm --prefix web install",
    "db:init": "npm --prefix api run db:init",
    "db:seed": "npm --prefix api run db:seed",
    "dev:api": "npm --prefix api run dev",
    "dev:web": "npm --prefix web run dev",
    "dev": "concurrently \"npm:dev:api\" \"npm:dev:web\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
